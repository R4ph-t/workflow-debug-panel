{
  "name": "workflow-debug-panel",
  "version": "0.1.0",
  "description": "Reusable React component for debugging Render Workflows in demo applications",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
